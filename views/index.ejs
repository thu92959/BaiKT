
<!DOCTYPE html>
<html>
  <head>
    <title>User 3</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <style>
      #tr1 {
        background-color: rgb(93, 124, 218); 
        color: white;
      }
      #tr2 {
        text-align: center;
        background-color: rgb(252, 252, 252);
      }
      th,td {
        padding: 3px 5px;
      }
    </style>
  </head>
  <body>
    <h1>User 3</h1>
    <a href="form-add"><button>Add</button></a>
    <table border="1">
      <tr id="tr1">
        <th>ID</th>
        <th>IdDistrict</th>
        <th>IdVillage</th>
        <th>Address</th>
        <th>IsGetNotification</th>
        <th>IsVerified</th>
        <th>IsGetOpen</th>
        <th>ACTION</th>
      </tr>
      <% User3.forEach(item =>{ %>
        <tr id="tr2">
          <td><%= item._id %></td>
          <td><%= item.idDistrict %></td>
          <td><%= item.idVillage %></td>
          <td><%= item.address %></td>
          <td><%= item.isGetNotification %></td>
          <td><%= item.isVerified %></td>
          <td><%= item.isGetOpen %></td>
          <td><a href="/form-update/<%= item._id %>" style="text-decoration: none;"><span style="color: fuchsia;">Update</span></a> || <a href="/form-delete/<%= item._id %>" style="text-decoration: none;">Delete</a></td>
        </tr>
        <% }) %>
    </table>
  </body>
</html>